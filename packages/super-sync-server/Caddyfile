{$DOMAIN} {
    reverse_proxy supersync:1900

    # Security headers
    header {
        X-Content-Type-Options nosniff
        X-Frame-Options DENY
        Referrer-Policy strict-origin-when-cross-origin
        -Server
    }

    # Enable compression
    encode gzip zstd

    # Logging
    log {
        output stdout
        format console
    }
}
